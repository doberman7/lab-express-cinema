<h1>movies</h1>
<div>

      {{#each movie}}
      <div class="movie-card">
          <img src="{{this.image}}" alt="Image Movie">
          <h3>{{this.title}}</h3>

          <button type="button" class="btn btn-light btn-lg">  <a href="/movies/{{_id}}">See more</a></button>

      </div>
      {{/each}}

  </div>
